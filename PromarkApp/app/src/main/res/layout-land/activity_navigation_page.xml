<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallpaper"
    tools:context="derek.promarkapp.NavigationPage"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/btnFilmList"
        android:layout_width="113dp"
        android:layout_height="99dp"
        android:text="Film List"
        android:background="@android:color/holo_blue_dark"
        android:layout_marginEnd="19dp"
        tools:layout_editor_absoluteY="38dp"
        tools:layout_editor_absoluteX="55dp" />

    <Button
        android:id="@+id/btnTimer"
        android:layout_width="116dp"
        android:layout_height="101dp"
        android:background="@android:color/holo_blue_dark"
        android:text="Download Logo"
        tools:layout_editor_absoluteY="38dp"
        tools:layout_editor_absoluteX="402dp" />

    <Button
        android:id="@+id/btnEmail"
        android:layout_width="115dp"
        android:layout_height="101dp"
        android:background="@android:color/holo_blue_dark"
        android:text="Send us an Email!"
        android:layout_marginEnd="30dp"
        tools:layout_editor_absoluteY="153dp"
        tools:layout_editor_absoluteX="145dp" />

    <Button
        android:id="@+id/btnGallery"
        android:layout_width="114dp"
        android:layout_height="99dp"
        android:background="@android:color/holo_blue_dark"
        android:text="Residential Gallery"
        android:layout_marginEnd="37dp"
        tools:layout_editor_absoluteY="38dp"
        tools:layout_editor_absoluteX="228dp" />

    <Button
        android:id="@+id/btnBacktoLogin"
        android:layout_width="565dp"
        android:layout_height="77dp"
        android:text="Back to Login"
        android:background="@android:color/holo_blue_dark"
        android:layout_marginStart="43dp"
        android:layout_marginEnd="43dp"
        tools:layout_editor_absoluteY="270dp"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/btnFindUs"
        android:layout_width="111dp"
        android:background="@android:color/holo_blue_dark"
        android:layout_height="100dp"
        android:text="Find Us!"
        tools:layout_editor_absoluteY="153dp"
        tools:layout_editor_absoluteX="317dp" />


</android.support.constraint.ConstraintLayout>
